package poo.clases;

public class Samsung extends SmartDevices {

    //Atributos
    boolean Explodes;

    //Constructores
    public Samsung(){}
    public Samsung(String nombre_comercial, String fabricante, String modelo, int year, double memoria, float size, boolean Explodes){
        super(nombre_comercial, fabricante, modelo, year, memoria, size);
        this.Explodes = Explodes;
    }

    @Override
    public String toString() {
        return "Samsung{" +
                ", nombre_comercial='" + nombre_comercial + '\'' +
                ", fabricante='" + fabricante + '\'' +
                ", modelo='" + modelo + '\'' +
                ", year=" + year +
                ", memoria=" + memoria +
                ", size=" + size +
                ", Explodes=" + Explodes +
                '}';
    }
}
